  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "login": "npm login",
    "publish_library": "npm publish --access public"
  },
  "devDependencies": {
    "vite": "^5.0.8"
  },
  "dependencies": {
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-fetch": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "lodash": "^4.17.21"
  }
