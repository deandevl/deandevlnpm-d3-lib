{
  "name": "@deandevlnpm/d3-lib",
  "version": "0.0.3",
  "description": "A collection of importable modules for creating various types of d3 charts",
  "repository": "https://github.com/deandevl/deandevlnpm-d3-lib",
  "author": "Rick Dean <deanr3@bardstown.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/d3-lib.umd.cjs",
